<div class="row">
	<div class="col-xs-8 col-xs-offset-2">
		<h1>Sign in</h1>
		<%= bootstrap_form_for @user, {url: session_path} do |f| %>
				<%= f.text_field :email %>
				<%= f.password_field :password %>
				<%= f.submit "Sign in", :class => "btn btn-primary" %>
		<% end %>
	</div>
</div>
